Таблица1
П1 



ВЫБРАТЬ 
	Таблица1.п1 КАК п1,
	Таблица1.п2 КАК п2,
	Таблица1.п3 КАК п3
ПОМЕСТИТЬ вт1
ИЗ
	Таблица1 КАК Таблица1


СГРУППИРОВАТЬ ПО
	Таблица1.П1,
	Таблица1.П2,
	Таблица1.П3
;

